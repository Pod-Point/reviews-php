{
  "name": "pod-point/reviews-php",
  "description": "A review service for PHP.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pod Point Software Team",
      "email": "software@pod-point.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "~5.3|~6.0",
    "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
    "illuminate/config": "5.5.*|5.6.*|5.7.*|5.8.*",
    "ext-json": "*",
    "vlucas/phpdotenv": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "PodPoint\\Reviews\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PodPoint\\Reviews\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "PodPoint\\Reviews\\Providers\\ReviewsProvider"
      ]
    }
  }
}
